    "def distance_3d(pt_A, pt_B):\n",
    "    x_del = pt_B[1] - pt_A[1];\n",
    "    y_del = pt_B[2] - pt_A[2];\n",
    "    z_del = pt_B[3] - pt_A[3];\n",
    "    leg = (x_del**2 + y_del**2 + z_del**2)**(1/2);\n",
    "    return(leg)"
   ]
  }
